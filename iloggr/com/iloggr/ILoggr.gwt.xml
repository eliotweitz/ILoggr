<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE module PUBLIC "-//Google Inc.//DTD Google Web Toolkit 1.6.4//EN" "http://google-web-toolkit.googlecode.com/svn/tags/1.6.4/distro-source/core/src/gwt-module.dtd">
<module rename-to="iloggr">
    <!-- Inherit the core Web Toolkit stuff.                        -->
    <inherits name="com.google.gwt.user.User"/>
    
    <!-- Inherit the default GWT style sheet.  You can change       -->
    <!-- the theme of your GWT application by uncommenting          -->
    <!-- any one of the following lines.                            -->
    <inherits name="com.google.gwt.user.theme.standard.Standard"/>
    <!-- <inherits name="com.google.gwt.user.theme.chrome.Chrome"/> -->
    <!-- <inherits name="com.google.gwt.user.theme.dark.Dark"/>     -->

    <!-- GWT-Ext -->
    <!-- <inherits name="com.gwtext.Charts"/> -->
    <!-- <inherits name="com.gwtext.ColumnTree"/> -->
    <!-- <inherits name="com.gwtext.GwtExt"/> -->
    <!-- <inherits name="com.gwtext.Pagebus"/> -->
    
    <!-- Google Visualization Library -->
    <inherits name="com.google.gwt.visualization.Visualization"/>
    
    <!--  GChart  -->
    <inherits name="com.googlecode.gchart.GChart"/>
    
    <!-- Google Maps Library 
    <inherits name="com.google.gwt.maps.GoogleMaps"/>
    -->
    <!--
        If you want to deploy this application outside of localhost,
        you must obtain a Google Maps API key at:
        http://www.google.com/apis/maps/signup.html
        Replace the src attribute below with a URL that contains your key.
    -->
    <!-- script src="http://maps.google.com/maps?gwt=1&amp;file=api&amp;v=2&amp;key=???" /-->
    
    <!-- You can usually run under localhost without a key 
    <script src="http://maps.google.com/maps?gwt=1&amp;file=api&amp;v=2" />
    -->
 
    <!-- GIN -->
    <inherits name="com.google.gwt.inject.Inject"/>

    <inherits name="com.iloggr.gwt.util.Util"/>
    
    <source path="client"/>
    <public path="public"/>
    
    <!-- Specify the app entry point class.                         -->
    <entry-point class="com.iloggr.client.ILoggrEntryPoint"/>
    
    <stylesheet src="js/ext/resources/css/ext-all.css" />
    <script src="js/ext/adapter/ext/ext-base.js" />
    <script src="js/ext/ext-all.js" />
</module>
